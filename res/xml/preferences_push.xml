<?xml version="1.0" encoding="utf-8"?>
 <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
     android:title="@string/preferences_interface_homescreen_general_category">
     
	<CheckBoxPreference android:key="com.t3hh4xx0r.haxlauncher.general_push_enable"
		android:title="@string/preferences_general_push_enable_title"
      	android:summary="@string/preferences_general_push_enable_summary"
      	android:defaultValue="true" />
	<CheckBoxPreference android:key="com.t3hh4xx0r.haxlauncher.general_push_enable_test"
		android:title="@string/preferences_general_push_enable_test_title"
		android:dependency="com.t3hh4xx0r.haxlauncher.general_push_enable"
      	android:summary="@string/preferences_general_push_enable_test_summary"
      	android:defaultValue="false" />	  
	<CheckBoxPreference android:key="com.t3hh4xx0r.haxlauncher.general_push_enable_updates"
		android:title="@string/preferences_general_push_enable_updates_title"
		android:dependency="com.t3hh4xx0r.haxlauncher.general_push_enable"
      	android:summary="@string/preferences_general_push_enable_updates_summary"
      	android:defaultValue="true" />	  
	<Preference android:key="com.t3hh4xx0r.haxlauncher.register_client_push"
		android:title="Enable Client Push Notifications"
      	android:summary="Enable push notifications sent from YOUR device!" /> 
	<Preference android:key="com.t3hh4xx0r.haxlauncher.view_client_push"
		android:title="View Client Push Notifications"
      	android:summary="Check out all push notifications from others and send your own!"
      	android:enabled="false" />       	            
 </PreferenceScreen>